{
  "name": "genealabs/laravel-maps",
  "description": "Eay - peasy map integration for Laravel.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpgmaps",
    "googlemaps",
    "biostall"
  ],
  "authors": [
    {
      "name": "Mike Bronner",
      "email": "mike@genealabs.com",
      "role": "developer"
    }
  ],
  "require": {
    "illuminate/support": "^10|^11"
  },
  "suggest": {
    "geocoder-php/GeocoderLaravel": "If you need to geocode addresses and don't already have marker coordinates, use this package. It provides geocoding functionality optimized for Laravel."
  },
  "autoload": {
    "psr-4": {
      "GeneaLabs\\LaravelMaps\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/testbench-browser-kit": "^8.3",
    "orchestra/testbench-dusk": "^8.3",
    "orchestra/testbench": "^8.3",
    "php-coveralls/php-coveralls" : "^2.5.3",
    "phpmd/phpmd": "^2.13.0",
    "phpunit/phpunit": "^10.0.19",
    "symfony/thanks": "^1.2"
  },
  "autoload-dev": {
    "psr-4": {
        "GeneaLabs\\LaravelMaps\\Tests\\": "tests/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "GeneaLabs\\LaravelMaps\\Providers\\Service"
          ],
          "aliases": {
              "Map": "GeneaLabs\\LaravelMaps\\Facades\\Map"
          }
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "symfony/thanks": true
    }
  }
}
